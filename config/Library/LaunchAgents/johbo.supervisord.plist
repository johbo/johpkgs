<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
 <plist version="1.0">
  <dict>
    <key>Label</key>
    <string>johbo.supervisord</string>
    <key>ProgramArguments</key>
    <array>
      <string>{{HOME}}/.nix-profile/bin/supervisord-with-profile.sh</string>
      <string>-c</string>
      <string>{{HOME}}/.nix-profile/etc/supervisord.conf</string>
    </array>
   <key>RunAtLoad</key>
   <true/>
   <key>ServiceDescription</key>
   <string>Supervisor Daemon</string>
  </dict>
</plist>
